{
  "name": "zhikariz/yii2-fixer",
  "description": "Yii2 Fixer - Code style fixer for Yii2 projects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Helmi Adi Prasetyo",
      "email": "helmi.prasetyo12@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "friendsofphp/php-cs-fixer": "^3.0",
    "symfony/console": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
